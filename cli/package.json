{
  "name": "protobuf-websocket-api",
  "version": "0.0.1",
  "description": "Write APIs for web front-end with protobuf.",
  "main": "src/index.js",
  "repository": "https://github.com/imdreamrunner/protobuf-websocket-api",
  "bin": {
    "pwa": "dist/cli.js"
  },
  "scripts": {
    "build": "npm run clean && npm run build-ts && npm run tslint && npm run copy-static",
    "build-ts": "tsc",
    "generate-schema": "pbjs -t static-module -w commonjs -o protobuf/schema.js protobuf/**.proto && pbts -o protobuf/schema.d.ts protobuf/schema.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json -e \"**/node_modules/**\"",
    "copy-static": "node copyStatics.js",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ivor Zhou",
  "license": "ISC",
  "devDependencies": {
    "@types/camelcase": "^4.1.0",
    "@types/uppercamelcase": "^3.0.0",
    "@types/shelljs": "^0.7.4",
    "shelljs": "^0.7.8",
    "tslint": "^5.7.0"
  },
  "dependencies": {
    "async-file": "^2.0.2",
    "camelcase": "^4.1.0",
    "uppercamelcase": "^3.0.0",
    "esprima": "^4.0.0",
    "mu2": "^0.5.21",
    "protobufjs": "^6.8.0",
    "stream-to-promise": "^2.2.0",
    "typescript": "^2.5.3"
  }
}
